.bingMsg{
  width:90%;
  background-color: #fff;
  margin-top: 100px;
  flex-direction: row;
  flex-wrap: wrap;
  display: flex;
}

.name{
  width:100%;
  background-color: #fff;
  text-align: center;
  font-size: 50rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.name::after{
  border: none;
}

.price{
  width:100%;
  background-color: rgba(195, 11, 241, 0.5);
  text-align: center;
  font-size: 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.price::after{
  border: none;
}

.userGroup{
  background-color: #fff;
  text-align: center;
  font-size: 50rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  background: none;
}

.userGroup::after{
  border: none;
}

.OK{
  margin-top: 20rpx;
  width: 90%;
}