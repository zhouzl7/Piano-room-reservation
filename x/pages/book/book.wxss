
page{
  background-color: #EDEDED;
  height: 100%;
  overflow: hidden;
}
.last{
  flex-direction: row;
  position: absolute;
  display: flex;
  width:100%;
  height:80rpx;
  bottom:0;
  
}
.Days {
  width: 100%;
  flex-direction: row;
  display: flex;
}
.DayBtn{
  border-bottom: 1rpx solid rgba(131, 128, 128, 0.521);
  width:100%;
  height: 12%;
  justify-content: center;
  border-radius: 0;
  border-width: 1rpx;
  background-color: #fff;
  font-size: 40rpx;
}
.DayBtn::after{
  border: none;
}


.Day1{
  height: 10rpx;
  width: 60%;
  border-color: #fff;
}
.Day1::after{
  border: none;
}
.container{
  margin-top: -200rpx;
  height: 130%;
  overflow: hidden;
}


.roomAndTime{
  margin-top: 2rpx;
  height: 90%;
  flex-direction: row;
  display: flex;
  width: 100%;
}

.Rooms {
  width: 33.3%;
  flex-direction: column;
  flex-wrap: wrap;
  display: flex;
  text-align: center;
}
.RoomBtn{
  height: 80rpx;
  line-height: 80rpx;
  justify-content: center;
  background-color: #fff;
}

.Times {
  width: 66.6%;
  flex-direction: row;
  flex-wrap: wrap;
  display: flex;
  line-height: 80rpx;
}

.Time{
  width: 100%;
  font-size: 30rpx;
  text-align: center;
  justify-content: center;
  border-radius: 0;
  border-width: 1rpx;
  }
  .Time::after{
  border: none;
}
.money{
  color:#fff;
  width:100%;
  justify-content: center;
  border-radius: 0;
  border-width: 1rpx;
  background-color: cornflowerblue;
  font-size: 36rpx;
}
.makesure{
  color:#fff;
  width:40%;
  justify-content: center;
  border-radius: 0;
  border-width: 1rpx;
  background-color: cornflowerblue;
  font-size: 36rpx;
}
.type{
  opacity: 0.5;
  width:20%;
  justify-content: center;
  border-radius: 0;
  border-width: 1rpx;
  font-size: 32rpx;
}
.nameImage, .keyImage {
  width: 40rpx;
  height: 40rpx;
  margin-top: 20rpx;
}
