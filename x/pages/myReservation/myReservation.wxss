.myres {
  width:90%;
  height:30px;
  background-color: #fff;
  font-size: 15px;
  line-height: 30px;
  text-align: center
}

.res{
  flex-direction: row;
  display: flex;
  justify-content: center;
  text-align: center;
}

.btn{
  display: flex;
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  justify-content: center;
}
.head{
  font-size:25px;
  font-weight: bold;
  text-align: center;
  
}
.content{
  
  font-size:15px;
  font-weight: lighter;
  line-height: 60rpx;
}
.circle {
  display: inline-block;
  margin-left:20px;
  margin-right:10px;
    width: 12px;
    height: 12px;
    background: cornflowerblue;
    border-radius: 50%;
}

.reservation{
  
  margin-left: 5%;
  margin-right: 5%;
  box-shadow: 1px 1px 1px;
  margin-top: 10px;
  text-align: left;
  background-color: #fff;
}



