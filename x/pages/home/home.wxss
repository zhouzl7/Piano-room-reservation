/* pages/hometest/home test.wxss */
.table {
  border-bottom: 1px solid rgba(169, 169, 169, 0.521);
}
.head{
  background-color: white;
  height: 50px;
}
.choose{
  margin-bottom: 5%;
  background-color: cornflowerblue;
  height: 10%;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
}
.td {
    color: #fff;
    margin-left: 5%;
}
.bg-w{
  background: white;
}
.bg-g{
  background: #E6F3F9;
}
.th {
  width: 50%;
  font-size: 18px;
  justify-content: center;
  display: flex;
  align-items: center;
  border-radius: 0;
}
.room{
  background-color: white;
  text-align: left;
}

.allroom{
  width:90%;
  background-color: #fff;
  margin-top: 10px;
}

.room_type{
  width:100%;
  background-color: #fff;
  text-align: center;
  font-size: 40rpx;
}
.room_type::after{
  border: none;
}

.room{
  width:100%;
  background-color: #fff;
  text-align: center;
  font-size: 40rpx;
}
.room::after{
  border: none;
}
